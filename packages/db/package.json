{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@polotrip/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.mjs",
      "types": "./dist/schema/index.d.ts"
    },
    "./models": {
      "import": "./dist/models/index.mjs",
      "types": "./dist/models/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@types/node": "~22.13.1",
    "drizzle-orm": "0.39.3",
    "drizzle-zod": "~0.7.0",
    "pg": "~8.13.1",
    "postgres": "3.4.4",
    "zod": "~3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "~9.4.0",
    "@polotrip/eslint-config": "workspace:*",
    "@polotrip/ts-config": "workspace:*",
    "drizzle-kit": "~0.30.4",
    "tsup": "8.2.4"
  }
}
